{
  "name": "car-rental",
  "version": "1.0.0",
  "description": "Car Rental application built with the MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "data:import": "node backend/config/seeder.js",
    "data:destroy": "node backend/config/seeder.js -d"
  },
  "author": "Irfan Shaikh",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bootstrap": "^5.3.2",
    "react-bootstrap": "^2.9.0",
    "react-input-range": "^1.3.0"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  }
}
